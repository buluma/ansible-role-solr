---
#
# Ansible managed
#
exclude_paths:
  - meta/preferences.yml
  - molecule/default/prepare.yml
  - molecule/default/converge.yml
  - molecule/default/collections.yml
  - .tox
  - .cache
  - .github
  - requirements.yml

skip_list:
  - yaml[truthy]
  - no-free-form
  - no-changed-when
  - fqcn[action-core]
  - risky-file-permissions

enable_list:
  - name[prefix]
